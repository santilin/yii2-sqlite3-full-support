{
    "name": "santilin/yii2-sqlite3-full-support",
    "type": "yii2-extension",
    "description": "Add extended support for sqlite driver for Yii2",
    "keywords": ["yii2","extension","sqlite"],
    "license": "LGPL-3.0+",
	"homepage": "https://github.com/santilin/yii2-sqlite3-full-support",
    "support": {
        "issues": "https://github.com/santilin/yii2-sqlite3-full-support/issues",
        "source": "https://github.com/santilin/yii2-sqlite3-full-support"
    },
    "authors": [
        {
            "name": "Santiago Capel Torres",
            "email": "software@noviolento.es"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.48"
    },
    "autoload": {
        "psr-4": {
            "santilin\\db\\": "src/db/"
        }
    },
    "extra": {
		"bootstrap": "santilin\\db\\Bootstrap",
		"version": "1.0.0"
	}
}

